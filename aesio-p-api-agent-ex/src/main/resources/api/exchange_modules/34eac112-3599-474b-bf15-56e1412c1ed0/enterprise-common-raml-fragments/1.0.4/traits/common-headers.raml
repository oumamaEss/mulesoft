#%RAML 1.0 Trait
headers:
  x-correlation-id:
    type: string
    description: UUID alphanumérique transmis à chaque requête à des fins de suivi transactionel
    required: true
    example: e393ef07-de7d-46c7-b514-27581be526d2
  x-source-system:
    type: string
    description: identification du système source
    required: false
    example: "ANTENIA_PROXY"